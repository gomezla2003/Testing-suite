        <name>TEST_Util</name>
        <script><![CDATA[var TEST_Util = Class.create();
TEST_Util.prototype = {
    initialize: function() {
    },

	getOrder: function(script){
		var order;
		var sc = new GlideRecord('x_sthc_testing_sui_test_step');
		sc.addQuery('test_script',script);
		sc.orderBy('order');
		sc.query();
		while(sc.next()){
			order = sc.order;
	
		}
		return order.toString();
		
	},
	
    type: 'TEST_Util'
};]]></script>
